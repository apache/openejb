<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 1.1//EN"
                         "http://java.sun.com/j2ee/dtds/ejb-jar_1_1.dtd">
<ejb-jar>
<enterprise-beans>
  <session>
    <description>The home page for the web admininstration</description>
    <ejb-name>webadmin/Home</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.HomeBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Allows the user to graphicly edit the OPENEJB_HOME/openejb.conf file</description>
    <ejb-name>webadmin/Configuration</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.config.ConfigBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>
    Lists all the system properties by calling:
    System.getProperties()
    </description>
    <ejb-name>webadmin/Properties</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.PropertiesBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>
    Lists the system log files
    </description>
    <ejb-name>webadmin/ListLogs</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.ListLogsBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>An online tool for deploying EJB's</description>
    <ejb-name>webadmin/Deployment</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.deploy.DeployBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Creates this list, and all the deployed beans</description>
    <ejb-name>webadmin/DeploymentList</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.deploy.DeploymentListBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>This bean does the deployment work along with the Deployment bean</description>
    <ejb-name>deploy/webadmin/Deployer</ejb-name>
    <home>org.openejb.admin.web.deploy.DeployerHome</home>
    <remote>org.openejb.admin.web.deploy.DeployerObject</remote>
    <ejb-class>org.openejb.admin.web.deploy.DeployerBean</ejb-class>
    <session-type>Stateful</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Holds the Openejb object in between HTTP requests</description>
    <ejb-name>config/webadmin/ConfigurationData</ejb-name>
    <home>org.openejb.admin.web.config.ConfigurationDataHome</home>
    <remote>org.openejb.admin.web.config.ConfigurationDataObject</remote>
    <ejb-class>org.openejb.admin.web.config.ConfigurationDataBean</ejb-class>
    <session-type>Stateful</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Allows the user to map CMP fields with database fields</description>
    <ejb-name>webadmin/CMPMapping</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.cmp.mapping.CMPMappingBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Holds the mapping info in between HTTP requests</description>
    <ejb-name>mapping/webadmin/CMPMappingData</ejb-name>
    <home>org.openejb.admin.web.cmp.mapping.CMPMappingDataHome</home>
    <remote>org.openejb.admin.web.cmp.mapping.CMPMappingDataObject</remote>
    <ejb-class>org.openejb.admin.web.cmp.mapping.CMPMappingDataBean</ejb-class>
    <session-type>Stateful</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
</enterprise-beans>
</ejb-jar>