<AppContext>
  <Import path="imported-app-ctx.xml" />
  <Configuration>
    foo.bar = dummy
    <AsynchronousPool>
      CorePoolSize = 10
      MaximumPoolSize = 10
    </AsynchronousPool>
    <AnyPropertyPrefix>
      someproperty = foo
    </AnyPropertyPrefix>
    <org.quartz>
      scheduler.instanceName = my-app
      scheduler.instanceId = my-bean
      threadPool.threadCount = 10
      threadPool.class = org.superbiz.MyCustomThreadPool
      plugin.LogPlugin.class = org.superbiz.MyLogPlugin
      plugin.LogPlugin.active = true
    </org.quartz>
    1.2 = 3
  </Configuration>
  <EnvEntries>
    <EnvEntry key="eek">eev</EnvEntry>
    <EnvEntry key="jee">openejb</EnvEntry>
  </EnvEntries>
  <BeanContexts>
    <BeanContext id="CalculatorBean">
      <Configuration>
        no.root = ok
        <cxf.jaxws>
          in-interceptors = wss4j
        </cxf.jaxws>
      </Configuration>
    </BeanContext>
  </BeanContexts>
  <ModuleContext id="2">
    <Properties>
      modulecontext = 2
    </Properties>
    <BeanContexts>
      <BeanContext id="BeanInAModule">
        <Configuration>
          module.id = mId
        </Configuration>
      </BeanContext>
    </BeanContexts>
    <Pojos>
      <Pojo id="org.foo2.bar">
        <Configuration>
          <cxf.jaxrs>
            module = 2
          </cxf.jaxrs>
        </Configuration>
      </Pojo>
    </Pojos>
  </ModuleContext>
  <Pojos>
    <Pojo id="org.foo.bar">
      <Configuration>
        <cxf.jaxrs>
          features = my-feature
        </cxf.jaxrs>
      </Configuration>
    </Pojo>
  </Pojos>
  <Resources>
    <Service id="wss4j" class-name="org.apache.openejb.server.cxf.config.WSS4JInInterceptorFactory" factory-name="create">
      action = UsernameToken
      passwordType = PasswordText
      passwordCallbackClass = org.superbiz.ws.security.PasswordCallbackHandler
    </Service>
    <Resource id="aDs" type="DataSource">
      JdbcUrl = notsureitwillconnectthisway
    </Resource>
  </Resources>
</AppContext>

